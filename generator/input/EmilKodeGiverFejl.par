Actor NumActor{
    State{
        double number;
    }
    Knows{
    NumActor testActor;
    }
    Spawn(double x){
        State.number = x;
    }
    on getSquared(){
        NumActor getSquaredActor = Spawn NumActor(2.2);
        double result;
        result = squared(State.number);

        getSquaredActor <- getSquared();
    }

    local squared(double num): double{
        return num*num;
    }
}

