
Script HelloWorld {
    on greet(String name);
}

main() {
    GreeterMain gm = Spawn GreeterMain();
    gm <- greet("World");
}

Actor GreeterMain follows HelloWorld {
    State {}
    Knows {}

    Spawn() {
        print("HelloWorld actor started");

    }

    on greet(String name) {
        print("Hello " + name + "!");
    }
}
